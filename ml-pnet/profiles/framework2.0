<?xml version="1.0"?>
<profile>
<!-- .NET Framework 1.1 profile -->

<!-- Install as the default version in the assembly cache -->
<define name="INSTALL_AS_DEFAULT" value="false"/>

<!-- Include .NET Framework 1.1 features -->
<define name="NET_1_1" value="true"/>

<!-- use only .NET 1.1 features (used by System.Data) -->
<define name="ONLY_1_1" value="true"/>

<!-- Include .NET Framework 2.0 features -->
<define name="NET_2_0" value="true"/>

</profile>
